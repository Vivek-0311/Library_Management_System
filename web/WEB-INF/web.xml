<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>vivek.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>log</servlet-name>
        <servlet-class>log</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newstudent</servlet-name>
        <servlet-class>newstudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newbook</servlet-name>
        <servlet-class>newbook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>return1</servlet-name>
        <servlet-class>return1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>issue</servlet-name>
        <servlet-class>issue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forget</servlet-name>
        <servlet-class>forget</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>statics</servlet-name>
        <servlet-class>statics</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>log</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newstudent</servlet-name>
        <url-pattern>/newstudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newbook</servlet-name>
        <url-pattern>/newbook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>return1</servlet-name>
        <url-pattern>/return1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>issue</servlet-name>
        <url-pattern>/issue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forget</servlet-name>
        <url-pattern>/forget</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>statics</servlet-name>
        <url-pattern>/statics</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
